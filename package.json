    "lodash": "^4.17.21",
    "mars3d": "^3.4.0",
    "monaco-editor": "^0.33.0",
    "nprogress": "^0.2.0",
    "react": "^18.1.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "split.js": "^1.6.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.22",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
