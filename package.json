    "typescript": "4.5.5",
    "vite": "^4.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vite-plugin-style-import": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marsgis/mars3d-react-example.git"
