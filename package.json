    "lodash": "^4.17.21",
    "mars3d": "~3.6.0",
    "mars3d-cesium": "~1.111.1",
    "monaco-editor": "^0.33.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "split.js": "^1.6.5",
    "uuid": "^8.3.2"
